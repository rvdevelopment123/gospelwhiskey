    "162922448038a251fb": {
      "type": "text-with-image",
      "settings": {
        "subheading": "",
        "title": "Common Ground",
        "image": "shopify:\/\/shop_images\/The_Gospel_Community_Charity_Partners_Common_Ground_968022ff-1b69-4019-b518-ac20eedb5fe1.jpg",
        "image_position": "right",
        "bg_colour": "rgba(0,0,0,0)",
        "content": "<p><em>Common Ground<\/em> work tirelessly to strengthen First Nations cultures and to challenge systems of power so that First Nations people can have control over their futures. They are a First Nations-led not-for-profit who amplify First Nations voices, knowledge and stories; campaign to make changes in the education and justice systems; and develop new ways of celebrating and strengthening culture so it can be shared with future generations.<br\/><br\/>Learn more <a href=\"https:\/\/shop.thegospelwhiskey.com\/blogs\/journal\/discover-our-community\" target=\"_blank\" title=\"Discover Our Community\">here<\/a>.<\/p>",
        "title2": "",
        "link_text": "",
        "link_url": "",
        "enable_email": false
      }
