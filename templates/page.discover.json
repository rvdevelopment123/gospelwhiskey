    "1629224474d4a0d99e": {
      "type": "text-with-image",
      "settings": {
        "subheading": "",
        "title": "WIRE",
        "image": "shopify:\/\/shop_images\/The_Gospel_Community_Charity_Partners_WIRE_011d3e69-dfc5-42ef-8cae-01bdf271efa2.jpg",
        "image_position": "left",
        "bg_colour": "rgba(0,0,0,0)",
        "content": "<p><em>WIRE<\/em> is a free information, support and referral service dedicated to supporting women, non-binary, and gender-diverse people across Victoria, working towards equality since 1983 centering the voices and stories of people in the community they support. <em>WIRE<\/em> assists with matters as wide reaching as gender equity, family violence, social isolation, employment, economic insecurity and homelessness.<br\/><br\/>Learn more <a href=\"https:\/\/shop.thegospelwhiskey.com\/blogs\/journal\/discover-our-community\" target=\"_blank\" title=\"Discover Our Community\">here<\/a>.<\/p>",
        "title2": "",
        "link_text": "",
