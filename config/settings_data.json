          "15d796c6-19d7-4aeb-945b-c316d1fbc765": {
            "type": "image",
            "settings": {
              "image": "shopify:\/\/shop_images\/The_Gospel_Straight_Rye_Whiskey.png",
              "desktop_video": "",
              "mobile_video": "",
              "apply_overlay": true,
              "content_position": "bottomLeft",
              "subheading": "",
              "title": "STRAIGHT RYE WHISKEY WINS GOLD",
              "title_mobile": "",
              "content": "<p>Our <em>Straight Rye Whiskey<\/em> won gold at the 2022 San Francisco World Spirits Competition - the highest awarded Australian Rye Whiskey.<br\/><br\/>The award is a testament to our constant pursuit of excellence, which is driven by a dedication to making a unique spirit from the most stubborn of grains - 100% Australian Mallee grown rye.<\/p>",
              "button_1_text": "SHOP NOW",
              "button_1_link": "shopify:\/\/products\/straight-rye-whiskey",
              "button_2_text": "",
              "button_2_link": "",
              "countryClass": ""
            }
          },
